<p id="notice"><%= notice %></p>

<h1>Ethers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ethers.each do |ether| %>
      <tr>
        <td><%= link_to 'Show', ether %></td>
        <td><%= link_to 'Edit', edit_ether_path(ether) %></td>
        <td><%= link_to 'Destroy', ether, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ether', new_ether_path %>
