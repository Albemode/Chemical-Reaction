<!DOCTYPE html>
<html>
  <head>
    <title>Chemical Reaction App</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
  </head>

  <body>
    <div id="navbarCollapse" class="collapse navbar-collapse">

        <ul class="nav navbar-nav">
          <li class="dropdown">
              <a data-toggle="dropdown" class="dropdown-toggle" href="#">Emotions<b class="caret"></b></a>
              <ul role="menu" class="dropdown-menu">

              <li><%= link_to "Anger", angers_path %></li>
              <li><%= link_to "Confusions", confusions_path %></li>
              <li><%= link_to "Fear", fears_path %></li>
              <li><%= link_to "Envy", envies_path %></li>
              <li><%= link_to "Hope", hopes_path %></li>
              <li><%= link_to "Love", loves_path %></li>
              <li><%= link_to "Pain", pains_path %></li>
              <li><%= link_to "Respect", respects_path %></li>
              <li><%= link_to "Saddness", saddnesses_path %></li>
              <li><%= link_to "Surpirse", surprises_path %></li>
              <li><%= link_to "Trust", trusts_path %></li>
              </ul>
          </li>

          <li class="dropdown">
              <a data-toggle="dropdown" class="dropdown-toggle" href="#">Drugs<b class="caret"></b></a>
              <ul role="menu" class="dropdown-menu">
                <li><%= link_to "Alcohol", alcohols_path %></li>
                <li><%= link_to "Amphetamines", amphetamines_path %></li>
                <li><%= link_to "Butylone", butylones_path %></li>
                <li><%= link_to "Cocaine", cocaines_path %></li>
                <li><%= link_to "Codeine", codeines_path %></li>
                <li><%= link_to "DMT", dmts_path %></li>
                <li><%= link_to "Ether", ethers_path %></li>
                <li><%= link_to "4-HO-Mpits", fourhompits_path %></li>
                <li><%= link_to "GHBS", ghbs_path %></li>
                <li><%= link_to "Ketamine", ketamines_path %></li>
                <li><%= link_to "LSD", lsds_path %></li>
                <li><%= link_to "MDAMA", mdamas_path %></li>
                <li><%= link_to "MXE", mxes_path %></li>
                <li><%= link_to "Nitrou", nitrous_path %></li>
              </ul>
          </li>

            <li class="dropdown">
                <a data-toggle="dropdown" class="dropdown-toggle" href="#">Senses<b class="caret"></b></a>
                <ul role="menu" class="dropdown-menu">

                </ul>
            </li>
        </ul>



    <nav id="navbg">
      <div class="logo">
        <h1>Chemical Reaction</h1>
      </div>
      <ul>
        <li><%= link_to "Emotion", emotions_path %></li>
        <li><%= link_to "Drugs", drugs_path %></li>
        <li><%= link_to "Senses", senses_path %></li>
      </ul>
      </div>
    </nav>
    <%= yield %>
  </body>
</html>
