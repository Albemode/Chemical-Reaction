<p id="notice"><%= notice %></p>

<h1>Mxes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mxes.each do |mx| %>
      <tr>
        <td><%= link_to 'Show', mx %></td>
        <td><%= link_to 'Edit', edit_mx_path(mx) %></td>
        <td><%= link_to 'Destroy', mx, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mx', new_mx_path %>
