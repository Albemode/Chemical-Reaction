<p id="notice"><%= notice %></p>

<h1>Anticipations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @anticipations.each do |anticipation| %>
      <tr>
        <td><%= link_to 'Show', anticipation %></td>
        <td><%= link_to 'Edit', edit_anticipation_path(anticipation) %></td>
        <td><%= link_to 'Destroy', anticipation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Anticipation', new_anticipation_path %>
