<p id="notice"><%= notice %></p>

<h1>Thcs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @thcs.each do |thc| %>
      <tr>
        <td><%= link_to 'Show', thc %></td>
        <td><%= link_to 'Edit', edit_thc_path(thc) %></td>
        <td><%= link_to 'Destroy', thc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Thc', new_thc_path %>
