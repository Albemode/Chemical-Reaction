<body class="butylone">

<p id="notice"><%= notice %></p>

<h1>Butylones</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @butylones.each do |butylone| %>
      <tr>
        <td><%= link_to 'Show', butylone %></td>
        <td><%= link_to 'Edit', edit_butylone_path(butylone) %></td>
        <td><%= link_to 'Destroy', butylone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Butylone', new_butylone_path %>
</body>
