<p id="notice"><%= notice %></p>

<h1>Fourhompits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fourhompits.each do |fourhompit| %>
      <tr>
        <td><%= link_to 'Show', fourhompit %></td>
        <td><%= link_to 'Edit', edit_fourhompit_path(fourhompit) %></td>
        <td><%= link_to 'Destroy', fourhompit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fourhompit', new_fourhompit_path %>
