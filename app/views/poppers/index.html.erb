<p id="notice"><%= notice %></p>

<h1>Poppers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poppers.each do |popper| %>
      <tr>
        <td><%= link_to 'Show', popper %></td>
        <td><%= link_to 'Edit', edit_popper_path(popper) %></td>
        <td><%= link_to 'Destroy', popper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Popper', new_popper_path %>
