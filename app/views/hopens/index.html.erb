<p id="notice"><%= notice %></p>

<h1>Hopens</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hopens.each do |hopen| %>
      <tr>
        <td><%= link_to 'Show', hopen %></td>
        <td><%= link_to 'Edit', edit_hopen_path(hopen) %></td>
        <td><%= link_to 'Destroy', hopen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hopen', new_hopen_path %>
