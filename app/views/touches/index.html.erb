<p id="notice"><%= notice %></p>

<h1>Touches</h1>

<table>
  <thead>
    <tr>
      <th>Subtype</th>
      <th>Duration</th>
      <th>Detection</th>
      <th>Effect</th>
      <th>Nervous system</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @touches.each do |touch| %>
      <tr>
        <td><%= touch.subtype %></td>
        <td><%= touch.duration %></td>
        <td><%= touch.detection %></td>
        <td><%= touch.effect %></td>
        <td><%= touch.nervous_system %></td>
        <td><%= link_to 'Show', touch %></td>
        <td><%= link_to 'Edit', edit_touch_path(touch) %></td>
        <td><%= link_to 'Destroy', touch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add Touch data', new_touch_path %>
